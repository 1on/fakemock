{
  "name": "er1z/fakemock",
  "description": "A library to provide mocking abilities for annotated class properties",
  "license": "mit",
  "require": {
    "php": "^7.1",
    "symfony/property-access": "^4.1",
    "doctrine/annotations": "^1.6",
    "fzaninotto/faker": "^1.8",
    "icomefromthenet/reverse-regex": "^0.1.0",
    "phpdocumentor/reflection-docblock": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "Er1z\\FakeMock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Er1z\\FakeMock\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "suggest": {
    "symfony/intl": "To get a correct country code for Country assert",
    "symfony/validator": "To enhance field options autodetection"
  }
}
